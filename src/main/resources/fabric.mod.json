{
  "schemaVersion": 1,
  "id": "superman",
  "version": "${version}",
  "name": "Superman Mod",
  "description": "A mod which adds superman abilites like lasereyes",
  "authors": [
    "NoRiskk"
  ],
  "contact": {
    "homepage": "https://github.com/copyandexecute/superman-mod",
    "issues": "https://github.com/copyandexecute/superman-mod/issues",
    "sources": "https://github.com/copyandexecute/superman-mod"
  },
  "license": "GPLv3+",
  "icon": "assets/superman/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "de.hglabor.superman.Manager"
      }
    ],
    "server": [
      {
        "adapter": "kotlin",
        "value": "de.hglabor.superman.Manager"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "de.hglabor.superman.Manager"
      }
    ]
  },
  "mixins": [
    "superman.mixins.json"
  ],
  "accessWidener": "superman.accesswidener",
  "depends": {
    "fabricloader": ">=0.14",
    "minecraft": "1.20.x",
    "java": ">=17",
    "fabric-language-kotlin": "*",
    "fabric": "*",
    "silk-core": "*"
  }
}
